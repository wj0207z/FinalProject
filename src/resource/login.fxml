<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="loginPane" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
    <children>
        <TextField fx:id="usernameField" layoutX="100.0" layoutY="100.0" prefHeight="25.0" prefWidth="200.0"/>
        <PasswordField fx:id="passwordField" layoutX="100.0" layoutY="150.0" prefHeight="25.0" prefWidth="200.0"/>
        <Button layoutX="150.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleLogin" text="Login"/>
    </children>
</AnchorPane>
